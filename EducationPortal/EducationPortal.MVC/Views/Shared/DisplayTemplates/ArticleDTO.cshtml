@model ArticleDTO

<div class="row justify-content-between">
    <div class="col-5">
        Статья
    </div>
    <div class="col-6">
        "@Model.Name"
    </div>
</div>
<div class="row justify-content-between">
    <div class="col-5">
        URL:
    </div>
    <div class="col-6">
        @Model.Url
    </div>
</div>
<div class="row justify-content-between">
    <div class="col-5">
        Дата издания:
    </div>
    <div class="col-6">
        @Model.PublicationDate
    </div>
</div>
<input type="hidden" class="material-item-id" value="@Model.Id" />

